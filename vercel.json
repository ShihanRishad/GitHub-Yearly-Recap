{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "functions": {
        "api/meta.ts": {
            "includeFiles": "index.html"
        }
    },
    "rewrites": [
        {
            "source": "/u/:username/:year",
            "destination": "/api/meta?username=:username&year=:year"
        },
        {
            "source": "/u/:username",
            "destination": "/api/meta?username=:username"
        },
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        }
    ]
}